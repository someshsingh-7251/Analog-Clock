<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Analogue Clock</title>
</head>
<body>
    <div class="clock-wrapper">
        <ol class="clock-container">
          <li>1</li>
          <li>2</li>
          <li>3</li>
          <li>4</li>
          <li>5</li>
          <li>6</li>
          <li>7</li>
          <li>8</li>
          <li>9</li>
          <li>10</li>
          <li>11</li>
          <li>12</li>
        </ol>
        <div id="hour"></div>
        <div id="min"></div>
        <div id="sec"></div>

        <div class="date">
            <div id="today"></div>
            <div id="todate"></div>
        </div>
    </div>
    <button id="link"><a href="https://instagram.com/officialsomeshchinkusingh?utm_medium=copy_link">Please Support My Instagram Account</a></button>
    <script src="Clock.js"></script>
</body>
</html>
